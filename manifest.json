{
    "manifest_version": 3,
    "name": "UI Capture Chrome Extension",
    "version": "1.0",
    "description": "Inspect UI elements and highlight them on the page.",
    "permissions": [
        "activeTab"
    ],
    "devtools_page": "devtools.html",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    }
}